	<update id="${id}" parameterMap="${paramMap}">
		UPDATE ${table}
		   SET ${col1} = #${prop1}:VARCHAR#,
		       ${col2} = #${prop2}:VARCHAR#,
		       ${col3} = #${prop3}:INTEGER#,
		       ${col4} = #${prop4}:VARCHAR#
		 WHERE ${where}
	</update>
